pages = {913--8},
pmid = {19525943},
publisher = {Nature Publishing Group},
title = {{Awake replay of remote experiences in the hippocampus.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2750914\&tool=pmcentrez\&rendertype=abstract},
volume = {12},
year = {2009}
}

