	Month = {Oct},
	Number = {44},
	Pages = {17845-50},
	Pmc = {PMC3497772},
	Pmid = {22822217},
	Title = {Protein folding kinetics and thermodynamics from atomistic simulation},
	Volume = {109},
	Year = {2012},
	Bdsk-Url-1 = {http://dx.doi.org/10.1073/pnas.1201811109}}


