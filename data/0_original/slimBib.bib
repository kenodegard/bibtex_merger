@article{dimaio2007,
	Abstract = {MOTIVATION: One bottleneck in high-throughput protein crystallography is interpreting an electron-density map, that is, fitting a molecular model to the 3D picture crystallography produces. Previously, we developed ACMI (Automatic Crystallographic Map Interpreter), an algorithm that uses a probabilistic model to infer an accurate protein backbone layout. Here, we use a sampling method known as particle filtering to produce a set of all-atom protein models. We use the output of ACMI to guide the particle filter's sampling, producing an accurate, physically feasible set of structures.
RESULTS: We test our algorithm on 10 poor-quality experimental density maps. We show that particle filtering produces accurate all-atom models, resulting in fewer chains, lower sidechain RMS error and reduced R factor, compared to simply placing the best-matching sidechains on ACMI's trace. We show that our approach produces a more accurate model than three leading methods--Textal, Resolve and ARP/WARP--in terms of main chain completeness, sidechain identification and crystallographic R factor.
AVAILABILITY: Source code and experimental density maps available at http://ftp.cs.wisc.edu/machine-learning/shavlik-group/programs/acmi/},
	Author = {DiMaio, Frank and Kondrashov, Dmitry A and Bitto, Eduard and Soni, Ameet and Bingman, Craig A and Phillips, Jr, George N and Shavlik, Jude W},
	Date-Added = {2013-02-16 23:13:30 +0000},
	Date-Modified = {2013-02-16 23:13:30 +0000},
	Doi = {10.1093/bioinformatics/btm480},
	Journal = {Bioinformatics},
	Journal-Full = {Bioinformatics (Oxford, England)},
	Mesh = {Absorptiometry, Photon; Algorithms; Computer Simulation; Crystallography, X-Ray; Filtration; Models, Chemical; Models, Molecular; Models, Statistical; Particle Size; Protein Conformation; Proteins},
	Month = {Nov},
	Number = {21},
	Pages = {2851-8},
	Pmc = {PMC2567142},
	Pmid = {17933855},
	Pst = {ppublish},
	Title = {Creating protein models from electron-density maps using particle-filtering methods},
	Volume = {23},
	Year = {2007},
	Bdsk-Url-1 = {http://dx.doi.org/10.1093/bioinformatics/btm480}}


