title = {{Potent and selective phosphopeptide mimetic prodrugs targeted to the Src homology 2 (SH2) domain of signal transducer and activator of transcription 3.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21486047},
volume = {54},
year = {2011}
}

